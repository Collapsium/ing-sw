generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Productos{
  id             Int              @id @default(autoincrement())
  marca          String?          @db.VarChar
  nombre         String?          @db.VarChar
  costo          Int?
  stock          Int?
}

model test{
  id    Int     @id @default(autoincrement())
  value     String
  numerito  Int

}




